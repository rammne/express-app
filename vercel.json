{
    "version": 2,
    "builds": [
        {
            "src": "/api/app.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["views/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/app.js"
        }
    ]
}